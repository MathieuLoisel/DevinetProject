<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.devinetproject">
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.PlayActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".activity.SelectListActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>

        <activity android:name=".activity.SelectLevelActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".activity.MyResultsDetailsActivity" >


        </activity>
        <activity android:name=".activity.MyResultsActivity" />
        <activity android:name=".activity.MyResultsGeneralActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".activity.SubmitActivity" />

        <activity android:name=".activity.SettingsActivity" android:theme="@style/Theme.AppCompat.NoActionBar"  />
        <activity android:name=".activity.AboutUsActivity" android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".activity.MainActivity" android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>


    </application>

</manifest>